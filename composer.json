{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/livewire-starter-kit",
    "type": "project",
    "description": "The official Laravel starter kit for Livewire.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "artesaos/seotools": "^1.3",
        "ebess/advanced-nova-media-library": "^5.1",
        "endroid/qr-code": "^6.0",
        "getbrevo/brevo-php": "^2.0",
        "goldoni/core-repositories": "dev-main",
        "goldoni/laravel-model-permissions": "dev-main",
        "goldoni/laravel-teams": "dev-main",
        "goldoni/laravel-virtual-wallet": "dev-main",
        "graham-campbell/markdown": "^16.0",
        "guzzlehttp/guzzle": "^7.10",
        "khsing/world": "^0.9.0",
        "laravel-lang/common": "^6.7",
        "laravel/framework": "^12.0",
        "laravel/nova": "^5.0",
        "laravel/pennant": "^1.18",
        "laravel/scout": "^10.19",
        "laravel/tinker": "^2.10.1",
        "laravel/vonage-notification-channel": "^3.3",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/flux": "^2.1.1",
        "livewire/flux-pro": "^2.3",
        "livewire/volt": "^1.7.0",
        "meilisearch/meilisearch-php": "^1.16",
        "nwidart/laravel-modules": "^12.0",
        "opcodesio/log-viewer": "^3.19",
        "propaganistas/laravel-phone": "^6.0",
        "sereny/nova-permissions": "^1.7",
        "spatie/image-optimizer": "^1.8",
        "spatie/laravel-medialibrary": "^11.15",
        "spatie/laravel-one-time-passwords": "^1.0",
        "spatie/laravel-permission": "^6.21",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/laravel-tags": "^4.10",
        "spatie/laravel-webhook-client": "^3.4",
        "spatie/nova-tags-field": "^5.0",
        "spatie/nova-translatable": "^4.1",
        "stevebauman/location": "^7.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^1.1",
        "laravel/nova-devtool": "^1.8",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.45",
        "laravel/telescope": "^5.11",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-drift": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/phpstan": "^2.1",
        "rector/rector": "^2.1",
        "rector/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Core\\": "core/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "rector": "vendor/bin/rector process --ansi",
        "rector:dry": "vendor/bin/rector process --dry-run --ansi",
        "rector:list": "vendor/bin/rector list-rules",
        "rector:ci": "vendor/bin/rector process --dry-run --no-progress --ansi --output-format=github",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "merge-plugin": {
            "include": [
                "Modules/*/composer.json"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "repositories": {
        "flux-pro": {
            "type": "composer",
            "url": "https://composer.fluxui.dev"
        },
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        "0": {
            "type": "path",
            "url": "packages/goldoni/laravel-model-permissions",
            "options": {
                "symlink": true
            }
        },
        "1": {
            "type": "path",
            "url": "packages/goldoni/laravel-virtual-wallet",
            "options": {
                "symlink": true
            }
        },
        "2": {
            "type": "vcs",
            "url": "https://github.com/fgoldoni/core-repositories.git"
        },
        "3": {
            "type": "vcs",
            "url": "https://github.com/fgoldoni/laravel-cart.git"
        },
        "4": {
            "type": "path",
            "url": "packages/goldoni/laravel-teams",
            "options": {
                "symlink": true
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
